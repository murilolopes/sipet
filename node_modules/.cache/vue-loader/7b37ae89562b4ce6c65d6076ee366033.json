   <!-- Date picker -->\n    <div class=\"col-lg-6\">\n      <div class=\"card-box\">\n        <h4 class=\"header-title mt-0 mb-1\">Flatpickr - Date picker</h4>\n        <p class=\"sub-header\">Examples of date and time pickers</p>\n\n        <div>\n          <div class=\"form-group mb-3\">\n            <label>Basic</label>\n            <flat-pickr\n              v-model=\"date\"\n              class=\"form-control\"\n              placeholder=\"Basic datepicker\"\n              name=\"date\"\n            ></flat-pickr>\n          </div>\n          <div class=\"form-group mb-3\">\n            <label>Date & Time</label>\n            <flat-pickr\n              v-model=\"dateTime\"\n              :config=\"dateTimePicker\"\n              class=\"form-control\"\n              placeholder=\"Date and Time\"\n            ></flat-pickr>\n          </div>\n          <div class=\"form-group mb-3\">\n            <label>Human-friendly Dates</label>\n            <flat-pickr\n              v-model=\"humanFriendly\"\n              :config=\"wrap\"\n              class=\"form-control\"\n              placeholder=\"October 9, 2018\"\n            ></flat-pickr>\n          </div>\n          <div class=\"form-group mb-3\">\n            <label>MinDate and MaxDate</label>\n            <flat-pickr\n              v-model=\"min\"\n              class=\"form-control\"\n              :config=\"minmax\"\n              placeholder=\"mindate - maxdate\"\n            ></flat-pickr>\n          </div>\n          <div class=\"form-group mb-3\">\n            <label>Inline Calendar</label>\n            <flat-pickr\n              v-model=\"inlinepicker\"\n              :config=\"inline\"\n              class=\"form-control\"\n            ></flat-pickr>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"col-lg-6\">\n      <div class=\"card\">\n        <div class=\"card-body\">\n          <h4 class=\"header-title mt-0 mb-1\">Flatpickr - Time Picker</h4>\n          <p class=\"sub-header\">A lightweight and powerful time picker</p>\n\n          <div class=\"form-group mb-3\">\n            <label>Basic</label>\n            <flat-pickr\n              v-model=\"time\"\n              :config=\"timePicker\"\n              class=\"form-control\"\n              placeholder=\"Basic timepicker\"\n            ></flat-pickr>\n          </div>\n          <div class=\"form-group mb-3\">\n            <label>24-hour Time Picker</label>\n            <flat-pickr\n              v-model=\"format24\"\n              :config=\"format\"\n              class=\"form-control\"\n              placeholder=\"16:21\"\n            ></flat-pickr>\n          </div>\n          <div class=\"form-group mb-3\">\n            <label>Time Picker w/ Limits</label>\n            <flat-pickr\n              v-model=\"limit\"\n              :config=\"timelimit\"\n              class=\"form-control\"\n              placeholder=\"Limits\"\n            ></flat-pickr>\n          </div>\n          <div class=\"form-group mb-3\">\n            <label>Preloading Time</label>\n            <flat-pickr\n              v-model=\"loadtime\"\n              :config=\"preload\"\n              class=\"form-control\"\n              placeholder=\"Pick a time\"\n            ></flat-pickr>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <div class=\"row\">\n    <div class=\"col-lg-6\">\n      <div class=\"card-box\">\n        <h4 class=\"header-title mt-0 mb-1\">Input Masks</h4>\n        <p class=\"sub-header\">\n          A simple masking for multiple types of input\n        </p>\n\n        <div class=\"row\">\n          <div class=\"col-md-6\">\n            <form action=\"#\">\n              <div class=\"form-group\">\n                <label>Date</label>\n                <input\n                  v-model=\"datemask\"\n                  v-mask=\"'##/##/####'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"DD/MM/YYYY\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>Hour</label>\n                <input\n                  v-model=\"hour\"\n                  v-mask=\"'##:##:##'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"HH:MM:SS\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>Date & Hour</label>\n                <input\n                  v-model=\"datetime\"\n                  v-mask=\"'##/##/#### ##:##:##'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\"\n                  >e.g \"DD/MM/YYYY HH:MM:SS\"</span\n                >\n              </div>\n              <div class=\"form-group\">\n                <label>ZIP Code</label>\n                <input\n                  v-model=\"zipcode\"\n                  v-mask=\"'#####-###'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"xxxxx-xxx\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>Crazy Zip Code</label>\n                <input\n                  v-model=\"crazyzip\"\n                  v-mask=\"'#-##-##-##'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"x-xx-xx-xx\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>Money</label>\n                <input\n                  v-model=\"money\"\n                  v-mask=\"'###.###.###.###.###,##'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"Your money\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>4 digit Group</label>\n                <input\n                  v-model=\"digit\"\n                  v-mask=\"'#,####,####,####'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\"\n                  >e.g. \"x,xxxx,xxxx,xxxx\"</span\n                >\n              </div>\n            </form>\n          </div>\n          <!-- end col -->\n\n          <div class=\"col-md-6\">\n            <form action=\"#\">\n              <div class=\"form-group\">\n                <label>Telephone</label>\n                <input\n                  v-model=\"telephone\"\n                  v-mask=\"'####-####'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"xxxx-xxxx\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>Telephone with Code Area</label>\n                <input\n                  v-model=\"areacode\"\n                  v-mask=\"'(##) ####-####'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"(xx) xxxx-xxxx\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>US Telephone</label>\n                <input\n                  v-model=\"ustelephone\"\n                  v-mask=\"'(###) ###-####'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"(xxx) xxx-xxxx\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>SÃ£o Paulo Celphones</label>\n                <input\n                  v-model=\"celphone\"\n                  v-mask=\"'(##) #####-####'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"(xx) xxxxx-xxxx\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>CPF</label>\n                <input\n                  v-model=\"cpf\"\n                  v-mask=\"'###.###.###-##'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"xxx.xxx.xxxx-xx\"</span>\n              </div>\n              <div class=\"form-group\">\n                <label>CNPJ</label>\n                <input\n                  v-model=\"cnpj\"\n                  v-mask=\"'##.###.###/####-##'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\"\n                  >e.g \"xx.xxx.xxx/xxxx-xx\"</span\n                >\n              </div>\n              <div class=\"form-group\">\n                <label>IP Address</label>\n                <input\n                  v-model=\"ipaddress\"\n                  v-mask=\"'###.###.###.###'\"\n                  type=\"text\"\n                  class=\"form-control\"\n                />\n                <span class=\"font-13 text-muted\">e.g \"xxx.xxx.xxx.xxx\"</span>\n              </div>\n            </form>\n          </div>\n          <!-- end col -->\n        </div>\n        <!-- end row -->\n      </div>\n    </div>\n    <div class=\"col-lg-6\">\n      <div class=\"card\">\n        <div class=\"card-body\">\n          <h4 class=\"header-title mt-0 mb-1\">MaxLength</h4>\n          <p class=\"sub-header\"\n            >Uses the HTML5 attribute \"maxlength\" to work.</p\n          >\n\n          <label class=\"mb-1 font-weight-medium text-muted\"\n            >Default usage</label\n          >\n          <p class=\"text-muted font-13\"\n            >The badge will show up by default when the remaining chars are 10\n            or less:</p\n          >\n          <input\n            v-model=\"maxlen\"\n            type=\"text\"\n            class=\"form-control\"\n            :maxlength=\"10\"\n          />\n          <div v-if=\"maxlen\" class=\"text-center\">\n            <p\n              class=\"badge position-absolute\"\n              :class=\"{\n                'badge-success': maxlen.length !== 10,\n                'badge-danger': maxlen.length === 10,\n              }\"\n              >{{ maxlen.length }} / 10</p\n            >\n          </div>\n\n          <div>\n            <label class=\"mb-1 mt-3 font-weight-medium text-muted\"\n              >Threshold value</label\n            >\n            <p class=\"text-muted font-13\">\n              Do you want the badge to show up when there are 20 chars or\n              less? Use the\n              <code>threshold</code>\n              option:\n            </p>\n            <input\n              v-model=\"data\"\n              type=\"text\"\n              :maxlength=\"25\"\n              class=\"form-control\"\n            />\n            <div class=\"text-center\">\n              <p\n                v-if=\"data\"\n                class=\"badge position-absolute\"\n                :class=\"{\n                  'badge-success': data.length !== 25,\n                  'badge-danger': data.length === 25,\n                }\"\n                >{{ data.length }} / 25</p\n              >\n            </div>\n          </div>\n\n          <div>\n            <label class=\"mb-1 mt-3 font-weight-medium text-muted\"\n              >All the options</label\n            >\n            <p class=\"text-muted font-13\">\n              Please note: if the\n              <code>alwaysShow</code> option is enabled, the\n              <code>threshold</code> option is ignored.\n            </p>\n            <input\n              v-model=\"option\"\n              type=\"text\"\n              class=\"form-control\"\n              :maxlength=\"25\"\n            />\n            <div class=\"text-center\">\n              <p\n                v-if=\"option\"\n                class=\"badge position-absolute\"\n                :class=\"{\n                  'badge-success': option.length !== 25,\n                  'badge-danger': option.length === 25,\n                }\"\n              >\n                You typed\n                {{ option.length }} out of 25 chars available.\n              </p>\n            </div>\n          </div>\n\n          <div>\n            <label class=\"mb-1 mt-3 font-weight-medium text-muted\"\n              >Textareas</label\n            >\n            <p class=\"text-muted font-13\"\n              >Bootstrap maxlength supports textarea as well as inputs. Even\n              on old IE.</p\n            >\n            <textarea\n              v-model=\"textarea\"\n              class=\"form-control\"\n              :maxlength=\"225\"\n              rows=\"3\"\n              placeholder=\"This textarea has a limit of 225 chars.\"\n            ></textarea>\n            <div class=\"text-center\">\n              <p\n                v-if=\"textarea\"\n                class=\"badge position-absolute\"\n                :class=\"{\n                  'badge-success': textarea.length !== 225,\n                  'badge-danger': textarea.length === 225,\n                }\"\n              >\n                {{ textarea.length }} / 225\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</Layout>\n",null]}