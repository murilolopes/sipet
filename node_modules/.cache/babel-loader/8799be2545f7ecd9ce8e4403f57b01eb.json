SidebarTheme: 'default'\n      });\n      this.changeLayoutWidth({\n        layoutWidth: 'default'\n      });\n      this.changeLayoutType({\n        layoutType: layout\n      });\n      this.hide();\n    },\n\n    changeTheme(theme) {\n      this.changeLayoutType({\n        layoutType: 'vertical'\n      });\n      this.changeLeftSidebarTheme({\n        leftSidebarTheme: theme\n      });\n      this.hide();\n    },\n\n    changeType(condensed) {\n      this.changeLayoutType({\n        layoutType: 'vertical'\n      });\n      this.changeLeftSidebarType({\n        leftSidebarType: condensed\n      });\n      this.hide();\n    },\n\n    changeWidth(boxed) {\n      this.changeLayoutType({\n        layoutType: 'vertical'\n      });\n      this.changeLayoutWidth({\n        layoutWidth: boxed\n      });\n      this.hide();\n    }\n\n  }\n};",{"version":3,"sources":["right-sidebar.vue"],"names":[],"mappings":"AACA,OAAA,mBAAA,MAAA,uBAAA;AACA,SAAA,YAAA,EAAA,aAAA,QAAA,gBAAA;AAEA;;;;AAGA,eAAA;AACA,EAAA,UAAA,EAAA;AAAA,IAAA;AAAA,GADA;;AAEA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,MAAA,EAAA;AACA,QAAA,OAAA,EAAA,KAAA,mBADA;AAEA,QAAA,UAAA,EAAA,KAAA,UAFA;AAGA,QAAA,MAAA,EAAA,CAAA,OAAA;AAHA,OADA;AAMA,MAAA,MAAA,EAAA,KAAA,MAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,GAAA,MAAA;AANA,KAAA;AAQA,GAXA;;AAYA,EAAA,QAAA,EAAA,EACA,GAAA;AADA,GAZA;AAeA,EAAA,OAAA,EAAA,EACA,GAAA,aADA;;AAEA,IAAA,IAAA,GAAA;AACA,WAAA,OAAA,CAAA,kBAAA;AACA,KAJA;;AAKA,IAAA,mBAAA,CAAA,CAAA,EAAA,EAAA,EAAA;AACA,UAAA,CAAA,CAAA,MAAA,IAAA,CAAA,CAAA,MAAA,CAAA,SAAA,CAAA,QAAA,CAAA,kBAAA,CAAA,EACA,KAAA,OAAA,CAAA,gBAAA;AACA,KARA;;AASA,IAAA,UAAA,CAAA,KAAA,EAAA,EAAA,EAAA;AACA,aAAA,CAAA,KAAA,CAAA,MAAA,CAAA,SAAA,CAAA,QAAA,CAAA,cAAA,CAAA;AACA,KAXA;;AAYA,IAAA,YAAA,CAAA,MAAA,EAAA;AACA,WAAA,sBAAA,CAAA;AAAA,QAAA,gBAAA,EAAA;AAAA,OAAA;AACA,WAAA,iBAAA,CAAA;AAAA,QAAA,WAAA,EAAA;AAAA,OAAA;AACA,WAAA,gBAAA,CAAA;AAAA,QAAA,UAAA,EAAA;AAAA,OAAA;AACA,WAAA,IAAA;AACA,KAjBA;;AAkBA,IAAA,WAAA,CAAA,KAAA,EAAA;AACA,WAAA,gBAAA,CAAA;AAAA,QAAA,UAAA,EAAA;AAAA,OAAA;AACA,