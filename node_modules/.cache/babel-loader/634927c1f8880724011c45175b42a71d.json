te: null,\n      dateTimePicker: {\n        enableTime: true,\n        dateFormat: 'd-m-Y H:i'\n      },\n      wrap: {\n        wrap: true,\n        altInput: true,\n        dateFormat: 'Y-m-d'\n      },\n      dateTime: null,\n      humanFriendly: null,\n      inlinepicker: +new Date(),\n      min: null,\n      time: null,\n      format24: null,\n      limit: null,\n      loadtime: null,\n      minmax: {\n        minDate: '2020-01',\n        maxDate: '2020-03'\n      },\n      inline: {\n        inline: true\n      },\n      timePicker: {\n        wrap: true,\n        enableTime: true,\n        noCalendar: true\n      },\n      format: {\n        noCalendar: true,\n        enableTime: true,\n        dateFormat: 'H:i'\n      },\n      timelimit: {\n        enableTime: true,\n        noCalendar: true,\n        dateFormat: 'H:i',\n        minDate: '16:00',\n        maxDate: '22:30'\n      },\n      preload: {\n        enableTime: true,\n        noCalendar: true,\n        dateFormat: 'H:i',\n        defaultDate: '01:45'\n      },\n      digit: '',\n      zipcode: '',\n      crazyzip: '',\n      money: '',\n      datemask: '',\n      hour: '',\n      datetime: '',\n      ipaddress: '',\n      cnpj: '',\n      cpf: '',\n      celphone: '',\n      ustelephone: '',\n      areacode: '',\n      telephone: '',\n      maxlen: '',\n      data: '',\n      option: '',\n      textarea: ''\n    };\n  }\n\n};",{"version":3,"sources":["advanced.vue"],"names":[],"mappings":"AACA,OAAA,SAAA,MAAA,iBAAA;AACA,OAAA,MAAA,MAAA,eAAA;AACA,OAAA,UAAA,MAAA,yBAAA;AAEA,OAAA,WAAA,MAAA,iBAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA;AACA,IAAA,KAAA,EAAA,gBADA;AAEA,IAAA,IAAA,EAAA,CAAA;AAAA,MAAA,IAAA,EAAA,aAAA;AAAA,MAAA,OAAA,EAAA,SAAA,CAAA;AAAA,KAAA;AAFA,GADA;AAKA,EAAA,UAAA,EAAA;AAAA,IAAA,MAAA;AAAA,IAAA,UAAA;AAAA,IAAA;AAAA,GALA;;AAMA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,gBADA;AAEA,MAAA,KAAA,EAAA,CACA;AACA,QAAA,IAAA,EAAA,QADA;AAEA,QAAA,IAAA,EAAA;AAFA,OADA,EAKA;AACA,QAAA,IAAA,EAAA,OADA;AAEA,QAAA,IAAA,EAAA;AAFA,OALA,EASA;AACA,QAAA,IAAA,EAAA,UADA;AAEA,QAAA,MAAA,EAAA;AAFA,OATA,CAFA;AAgBA,MAAA,KAAA,EAAA,IAhBA;AAiBA,MAAA,MAAA,EAAA,IAjBA;AAkBA,MAAA,OAAA,EAAA,CACA,QADA,EAEA,QAFA,EAGA,YAHA,EAIA,QAJA,EAKA,QALA,EAMA,YANA,EAOA,SAPA,EAQA,UARA,EASA,OATA,EAUA,SAVA,EAWA,UAXA,EAYA,YAZA,EAaA,cAbA,EAcA,MAdA,EAeA,SAfA,EAgBA,SAhBA,EAiBA,UAjBA,EAkBA,UAlBA,EAmBA,MAnBA,CAlBA;AAuCA,MAAA,IAAA,EAAA,IAvCA;AAwCA,MAAA,cAAA,EAAA;AACA,QAAA,UAAA,EAAA,IADA;AAEA,QAAA,UAAA,EAAA;AAFA,OAxCA;AA4CA,MAAA,IAAA,EAAA;AACA,QAAA,IAAA,EAAA,IADA;AAEA,QAAA,QAAA,EAAA,IAFA;AAGA,QAAA,UAAA,EAAA;AAHA,OA5CA;AAiDA,MAAA,QAAA,EAAA,IAjDA;AAkDA,MAAA,aAAA,EAAA,IAlDA;AAmDA,MAAA,YAAA,EAAA,CAAA,IAAA,IAAA,EAnDA;AAoDA,MAAA,GAAA,EAAA,IApDA;AAqDA,MAAA,IAAA,EAAA,IArDA;AAsDA,MAAA,QAAA,EAAA,IAtDA;AAuDA,MAAA,KAAA,EAAA,IAvDA;AAwDA,MAAA,QAAA,EAAA,IAxDA;AAyDA,MAAA,MAAA,EAAA;AACA,QAAA,OAAA,EAAA,SADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAzDA;AA6DA,MAAA,MAAA,EAAA;AACA,QAAA,MAAA,EAAA;AADA,OA7DA;AAgEA,MAAA,UAAA,EAAA;AACA,QAAA,IAAA,EAAA,IADA;AAEA,QAAA,UAAA,EAAA,IAFA;AAGA,QAAA,UAAA,EAAA;AAHA,OAhEA;AAqEA,MAAA,MAAA,EAAA;AACA,QAAA,UAAA,EAAA,IADA;AAEA,QAAA,UAAA,EAAA,IAFA;AAGA,QAAA,UAAA,EAAA;AAHA,OArEA;AA0EA,MAAA,SAAA,EAAA;AACA,QAAA,UAAA,EAAA,IADA;AAEA,QAAA,UAAA,EAAA,IAFA;AAGA,QAAA,UAAA,EAAA,KAHA;AAIA,QAAA,OAAA,EAAA,OAJA;AAKA,QAAA,OAAA,EAAA;AALA,OA1EA;AAiFA,MAAA,OAAA,EAAA;AACA,QAAA,UAAA,EAAA,IADA;AAEA,QAAA,UAAA,EAAA,IAFA;AAGA,QAAA,UAAA,EAAA,KAHA;AAIA,QAAA,WAAA,EAAA;AAJA,OAjFA;AAuFA,MAAA,KAAA,EAAA,EAvFA;AAwFA,MAAA,OAAA,EAAA,EAxFA;AAyFA,MAAA,QAAA,EAAA,EAzFA;AA0FA,MAAA,KAAA,EAAA,EA1FA;AA2FA,MAAA,QAAA,EAAA,EA3FA;AA4FA,MAAA,IAAA,EAAA,EA5FA;AA6FA,MAAA,QAAA,EAAA,EA7FA;AA8FA,MAAA,SAAA,EAAA,EA9FA;AA+FA,MAAA,IAAA,EAAA,EA/FA;AAgGA,MAAA,GAAA,EAAA,EAhGA;AAiGA,MAAA,QAAA,EAAA,EAjGA;AAkGA,MAAA,WAAA,EAAA,EAlGA;AAmGA,MAAA,QAAA,EAAA,EAnGA;AAoGA,MAAA,SAAA,EAAA,EApGA;AAqGA,MAAA,MAAA,EAAA,EArGA;AAsGA,MAAA,IAAA,EAAA,EAtGA;AAuGA,MAAA,MAAA,EAAA,EAvGA;AAwGA,MAAA,QAAA,EAAA;AAxGA,KAAA;AA0GA;;AAjHA,CAAA","sourcesContent":["<script>\nimport appConfig from '@src/app.config'\nimport Layout from '@layouts/main'\nimport PageHeader from '@components/page-header'\n\nimport Multiselect from 'vue-multiselect'\n\nexport default {\n  page: {\n    title: 'Advanced Forms',\n    meta: [{ name: 'description', content: appConfig.description }],\n  },\n  components: { Layout, PageHeader, Multiselect },\n  data() {\n    return {\n      title: 'Advanced Forms',\n      items: [\n        {\n          text: 'Shreyu',\n          href: '/',\n        },\n        {\n          text: 'Forms',\n          href: '/',\n        },\n        {\n          text: 'Advanced',\n          active: true,\n        },\n      ],\n      value: null,\n      value1: null,\n      options: [\n        'Alaska',\n        'Hawaii',\n        'California',\n        'Nevada',\n        'Oregon',\n        'Washington',\n        'Arizona',\n        'Colorado',\n        'Idaho',\n        'Montana',\n        'Nebraska',\n        'New Mexico',\n        'North Dakota',\n        'Utah',\n        'Wyoming',\n        'Alabama',\n        'Arkansas',\n        'Illinois',\n        'Iowa',\n      ],\n      date: null,\n      dateTimePicker: {\n        enableTime: true,\n        dateFormat: 'd-m-Y H:i',\n      },\n      wrap: {\n        wrap: true,\n        altInput: true,\n        dateFormat: 'Y-m-d',\n      },\n      dateTime: null,\n      humanFriendly: null,\n      inlinepicker: +new Date(),\n      min: null,\n      time: null,\n      format24: null,\n      limit: null,\n      loadtime: null,\n      minmax: {\n        minDate: '2020-01',\n        maxDate: '2020-03',\n      },\n      inline: {\n        inline: true,\n      },\n      timePicker: {\n        wrap: true,\n        enableTime: true,\n        noCalendar: true,\n      },\n      format: {\n        noCalendar: true,\n        enableTime: true,\n        dateFormat: 'H:i',\n      },\n      timelimit: {\n        enableTime: true,\n        noCalendar: true,\n        dateFormat: 'H:i',\n        minDate: '16:00',\n        maxDate: '22:30',\n      },\n      preload: {\n        enableTime: true,\n        noCalendar: true,\n        dateFormat: 'H:i',\n        defaultDate: '01:45',\n      },\n      digit: '',\n      zipcode: '',\n      crazyzip: '',\n      money: '',\n      datemask: '',\n      hour: '',\n      datetime: '',\n      ipaddress: '',\n      cnpj: '',\n      cpf: '',\n      celphone: '',\n      ustelephone: '',\n      areacode: '',\n      telephone: '',\n      maxlen: '',\n      data: '',\n      option: '',\n      textarea: '',\n    }\n  },\n}\n</script>\n\n<template>\n  <Layout>\n    <PageHeader :title=\"title\" :items=\"items\" />\n    <!-- select -->\n    <div class=\"row\">\n      <div class=\"col-12\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"header-title mt-0 mb-1\">Select</h4>\n            <p class=\"sub-header\"\n              >Vue Multiselect - A custom selection solution for Vue JS.</p\n            >\n\n            <div class=\"row\">\n              <div class=\"col-xl-6 col-sm-6\">\n                <div class=\"form-group mt-3 mt-sm-0\">\n                  <label for=\"default\">Default</label>\n                  <multiselect v-model=\"value\" :options=\"options\"></multiselect>\n                </div>\n              </div>\n              <div class=\"col-xl-6 col-sm-6\">\n                <div class=\"form-group mt-3 mt-sm-0\">\n                  <label for=\"default\">Multiple select</label>\n                  <multiselect\n                    v-model=\"value1\"\n                    :options=\"options\"\n                    :multiple=\"true\"\n                  ></multiselect>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- end row -->\n    <div class=\"row\">\n      <!-- Date picker -->\n      <div class=\"col-lg-6\">\n        <div class=\"card-box\">\n          <h4 class=\"header-title mt-0 mb-1\">Flatpickr - Date picker</h4>\n          <p class=\"sub-header\">Examples of date and time pickers</p>\n\n          <div>\n            <div class=\"form-group mb-3\">\n              <label>Basic</label>\n              <flat-pickr\n                v-model=\"date\"\n                class=\"form-control\"\n                placeholder=\"Basic datepicker\"\n                name=\"date\"\n              ></flat-pickr>\n            </div>\n            <div class=\"form-group mb-3\">\n              <label>Date & Time</label>\n              <flat-pickr\n                v-model=\"dateTime\"\n                :config=\"dateTimePicker\"\n                class=\"form-control\"\n                placeholder=\"Date and Time\"\n              ></flat-pickr>\n            </div>\n            <div class=\"form-group mb-3\">\n              <label>Human-friendly Dates</label>\n              <flat-pickr\n                v-model=\"humanFriendly\"\n                :config=\"wrap\"\n                class=\"form-control\"\n                placeholder=\"October 9, 2018\"\n              ></flat-pickr>\n            </div>\n            <div class=\"form-group mb-3\">\n              <label>MinDate and MaxDate</label>\n              <flat-pickr\n                v-model=\"min\"\n                class=\"form-control\"\n                :config=\"minmax\"\n                placeholder=\"mindate - maxdate\"\n              ></flat-pickr>\n            </div>\n            <div class=\"form-group mb-3\">\n              <label>Inline Calendar</label>\n              <flat-pickr\n                v-model=\"inlinepicker\"\n                :config=\"inline\"\n                class=\"form-control\"\n              ></flat-pickr>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"header-title mt-0 mb-1\">Flatpickr - Time Picker</h4>\n            <p class=\"sub-header\">A lightweight and powerful time picker</p>\n\n            <div class=\"form-group mb-3\">\n              <label>Basic</label>\n              <flat-pickr\n                v-model=\"time\"\n                :config=\"timePicker\"\n                class=\"form-control\"\n                placeholder=\"Basic timepicker\"\n              ></flat-pickr>\n            </div>\n            <div class=\"form-group mb-3\">\n              <label>24-hour Time Picker</label>\n              <flat-pickr\n                v-model=\"format24\"\n                :config=\"format\"\n                class=\"form-control\"\n                placeholder=\"16:21\"\n              ></flat-pickr>\n            </div>\n            <div class=\"form-group mb-3\">\n              <label>Time Picker w/ Limits</label>\n              <flat-pickr\n                v-model=\"limit\"\n                :config=\"timelimit\"\n                class=\"form-control\"\n                placeholder=\"Limits\"\n              ></flat-pickr>\n            </div>\n            <div class=\"form-group mb-3\">\n              <label>Preloading Time</label>\n              <flat-pickr\n                v-model=\"loadtime\"\n                :config=\"preload\"\n                class=\"form-control\"\n                placeholder=\"Pick a time\"\n              ></flat-pickr>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-lg-6\">\n        <div class=\"card-box\">\n          <h4 class=\"header-title mt-0 mb-1\">Input Masks</h4>\n          <p class=\"sub-header\">\n            A simple masking for multiple types of input\n          </p>\n\n          <div class=\"row\">\n            <div class=\"col-md-6\">\n              <form action=\"#\">\n                <div class=\"form-group\">\n                  <label>Date</label>\n                  <input\n                    v-model=\"datemask\"\n                    v-mask=\"'##/##/####'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"DD/MM/YYYY\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>Hour</label>\n                  <input\n                    v-model=\"hour\"\n                    v-mask=\"'##:##:##'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"HH:MM:SS\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>Date & Hour</label>\n                  <input\n                    v-model=\"datetime\"\n                    v-mask=\"'##/##/#### ##:##:##'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\"\n                    >e.g \"DD/MM/YYYY HH:MM:SS\"</span\n                  >\n                </div>\n                <div class=\"form-group\">\n                  <label>ZIP Code</label>\n                  <input\n                    v-model=\"zipcode\"\n                    v-mask=\"'#####-###'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"xxxxx-xxx\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>Crazy Zip Code</label>\n                  <input\n                    v-model=\"crazyzip\"\n                    v-mask=\"'#-##-##-##'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"x-xx-xx-xx\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>Money</label>\n                  <input\n                    v-model=\"money\"\n                    v-mask=\"'###.###.###.###.###,##'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"Your money\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>4 digit Group</label>\n                  <input\n                    v-model=\"digit\"\n                    v-mask=\"'#,####,####,####'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\"\n                    >e.g. \"x,xxxx,xxxx,xxxx\"</span\n                  >\n                </div>\n              </form>\n            </div>\n            <!-- end col -->\n\n            <div class=\"col-md-6\">\n              <form action=\"#\">\n                <div class=\"form-group\">\n                  <label>Telephone</label>\n                  <input\n                    v-model=\"telephone\"\n                    v-mask=\"'####-####'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"xxxx-xxxx\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>Telephone with Code Area</label>\n                  <input\n                    v-model=\"areacode\"\n                    v-mask=\"'(##) ####-####'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"(xx) xxxx-xxxx\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>US Telephone</label>\n                  <input\n                    v-model=\"ustelephone\"\n                    v-mask=\"'(###) ###-####'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"(xxx) xxx-xxxx\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>SÃ£o Paulo Celphones</label>\n                  <input\n                    v-model=\"celphone\"\n                    v-mask=\"'(##) #####-####'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"(xx) xxxxx-xxxx\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>CPF</label>\n                  <input\n                    v-model=\"cpf\"\n                    v-mask=\"'###.###.###-##'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"xxx.xxx.xxxx-xx\"</span>\n                </div>\n                <div class=\"form-group\">\n                  <label>CNPJ</label>\n                  <input\n                    v-model=\"cnpj\"\n                    v-mask=\"'##.###.###/####-##'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\"\n                    >e.g \"xx.xxx.xxx/xxxx-xx\"</span\n                  >\n                </div>\n                <div class=\"form-group\">\n                  <label>IP Address</label>\n                  <input\n                    v-model=\"ipaddress\"\n                    v-mask=\"'###.###.###.###'\"\n                    type=\"text\"\n                    class=\"form-control\"\n                  />\n                  <span class=\"font-13 text-muted\">e.g \"xxx.xxx.xxx.xxx\"</span>\n                </div>\n              </form>\n            </div>\n            <!-- end col -->\n          </div>\n          <!-- end row -->\n        </div>\n      </div>\n      <div class=\"col-lg-6\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"header-title mt-0 mb-1\">MaxLength</h4>\n            <p class=\"sub-header\"\n              >Uses the HTML5 attribute \"maxlength\" to work.</p\n            >\n\n            <label class=\"mb-1 font-weight-medium text-muted\"\n              >Default usage</label\n            >\n            <p class=\"text-muted font-13\"\n              >The badge will show up by default when the remaining chars are 10\n              or less:</p\n            >\n            <input\n              v-model=\"maxlen\"\n              type=\"text\"\n              class=\"form-control\"\n              :maxlength=\"10\"\n            />\n            <div v-if=\"maxlen\" class=\"text-center\">\n              <p\n                class=\"badge position-absolute\"\n                :class=\"{\n                  'badge-success': maxlen.length !== 10,\n                  'badge-danger': maxlen.length === 10,\n                }\"\n                >{{ maxlen.length }} / 10</p\n              >\n            </div>\n\n            <div>\n              <label class=\"mb-1 mt-3 font-weight-medium text-muted\"\n                >Threshold value</label\n              >\n              <p class=\"text-muted font-13\">\n                Do you want the badge to show up when there are 20 chars or\n                less? Use the\n                <code>threshold</code>\n                option:\n              </p>\n              <input\n                v-model=\"data\"\n                type=\"text\"\n                :maxlength=\"25\"\n                class=\"form-control\"\n              />\n              <div class=\"text-center\">\n                <p\n                  v-if=\"data\"\n                  class=\"badge position-absolute\"\n                  :class=\"{\n                    'badge-success': data.length !== 25,\n                    'badge-danger': data.length === 25,\n                  }\"\n                  >{{ data.length }} / 25</p\n                >\n              </div>\n            </div>\n\n            <div>\n              <label class=\"mb-1 mt-3 font-weight-medium text-muted\"\n                >All the options</label\n              >\n              <p class=\"text-muted font-13\">\n                Please note: if the\n                <code>alwaysShow</code> option is enabled, the\n                <code>threshold</code> option is ignored.\n              </p>\n              <input\n                v-model=\"option\"\n                type=\"text\"\n                class=\"form-control\"\n                :maxlength=\"25\"\n              />\n              <div class=\"text-center\">\n                <p\n                  v-if=\"option\"\n                  class=\"badge position-absolute\"\n                  :class=\"{\n                    'badge-success': option.length !== 25,\n                    'badge-danger': option.length === 25,\n                  }\"\n                >\n                  You typed\n                  {{ option.length }} out of 25 chars available.\n                </p>\n              </div>\n            </div>\n\n            <div>\n              <label class=\"mb-1 mt-3 font-weight-medium text-muted\"\n                >Textareas</label\n              >\n              <p class=\"text-muted font-13\"\n                >Bootstrap maxlength supports textarea as well as inputs. Even\n                on old IE.</p\n              >\n              <textarea\n                v-model=\"textarea\"\n                class=\"form-control\"\n                :maxlength=\"225\"\n                rows=\"3\"\n                placeholder=\"This textarea has a limit of 225 chars.\"\n              ></textarea>\n              <div class=\"text-center\">\n                <p\n                  v-if=\"textarea\"\n                  class=\"badge position-absolute\"\n                  :class=\"{\n                    'badge-success': textarea.length !== 225,\n                    'badge-danger': textarea.length === 225,\n                  }\"\n                >\n                  {{ textarea.length }} / 225\n                </p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </Layout>\n</template>\n"],"sourceRoot":"src/router/views/pages/ui/forms"}]}