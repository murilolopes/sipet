 var id = this.messages.length;\n        const message = this.chats.message;\n        const currentDate = new Date(); // Message Push in Chat\n\n        this.messages.push({\n          image: require('@assets/images/users/avatar-7.jpg'),\n          id: id + 1,\n          name: 'Shreyu',\n          message,\n          time: currentDate.getHours() + ':' + currentDate.getMinutes()\n        });\n      }\n\n      this.submitform = false;\n      this.chats = {};\n    }\n\n  }\n};",{"version":3,"sources":["chat.vue"],"names":[],"mappings":"AACA,OAAA,mBAAA,MAAA,uBAAA;AACA,SAAA,QAAA,QAAA,0BAAA;AAEA;;;;AAGA,eAAA;AACA,EAAA,UAAA,EAAA;AAAA,IAAA;AAAA,GADA;AAEA,EAAA,KAAA,EAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA;AAFA,KADA;AAKA,IAAA,gBAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA;AAFA,KALA;AASA,IAAA,QAAA,EAAA;AACA,MAAA,IAAA,EAAA,KADA;AAEA,MAAA,OAAA,EAAA,YAAA;AACA,eAAA,EAAA;AACA;AAJA;AATA,GAFA;;AAkBA,EAAA,IAAA,GAAA;AACA,WAAA;AACA,MAAA,KAAA,EAAA;AACA,QAAA,OAAA,EAAA;AADA,OADA;AAIA,MAAA,UAAA,EAAA;AAJA,KAAA;AAMA,GAzBA;;AA0BA,EAAA,WAAA,EAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,OAAA,EAAA;AAAA,QAAA;AAAA;AADA;AADA,GA1BA;AA+BA,EAAA,OAAA,EAAA;AACA;;;AAGA,IAAA,WAAA,GAAA;AACA,WAAA,UAAA,GAAA,IAAA,CADA,CAGA;;AACA,WAAA,EAAA,CAAA,MAAA;;AACA,UAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AACA;AACA,OAFA,MAEA;AACA,YAAA,EAAA,GAAA,KAAA,QAAA,CAAA,MAAA;AACA,cAAA,OAAA,GAAA,KAAA,KAAA,CAAA,OAAA;AACA,cAAA,WAAA,GAAA,IAAA,IAAA,EAAA,CAHA,CAKA;;AACA,aAAA,QAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,EAAA,OAAA,CAAA,mCAAA,CADA;AAEA,UAAA,EAAA,EAAA,EAAA,GAAA,CAFA;AAGA,UAAA,IAAA,EAAA,QAHA;AAIA,UAAA,OAJA;AAKA,UAAA,IAAA,EAAA,WAAA,CAAA,QAAA,KAAA,GAAA,GAAA,WAAA,CAAA,UAAA;AALA,SAAA;AAOA;;AACA,WAAA,UAAA,GAAA,KAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA;;AA3BA;AA/BA,CAAA","sourcesContent":["<script>\nimport VuePerfectScrollbar from 'vue-perfect-scrollbar'\nimport { required } from 'vuelidate/lib/validators'\n\n/**\n * Chat list - Renders a chat list with a form to enter new message\n */\nexport default {\n\tcomponents: { VuePerfectScrollbar },\n\tprops: {\n\t\ttitle: {\n\t\t\ttype: String,\n\t\t\tdefault: 'Chat',\n\t\t},\n\t\tchatWindowHeight: {\n\t\t\ttype: String,\n\t\t\tdefault: '230',\n\t\t},\n\t\tmessages: {\n\t\t\ttype: Array,\n\t\t\tdefault: function() {\n\t\t\t\treturn []\n\t\t\t},\n\t\t},\n\t},\n\tdata() {\n\t\treturn {\n\t\t\tchats: {\n\t\t\t\tmessage: '',\n\t\t\t},\n\t\t\tsubmitform: false,\n\t\t}\n\t},\n\tvalidations: {\n\t\tchats: {\n\t\t\tmessage: { required },\n\t\t},\n\t},\n\tmethods: {\n\t\t/**\n\t\t * Chat form submit\n\t\t */\n\t\tsaveMessage() {\n\t\t\tthis.submitform = true\n\n\t\t\t// stop here if form is invalid\n\t\t\tthis.$v.$touch()\n\t\t\tif (this.$v.$invalid) {\n\t\t\t\treturn\n\t\t\t} else {\n\t\t\t\tvar id = this.messages.length\n\t\t\t\tconst message = this.chats.message\n\t\t\t\tconst currentDate = new Date()\n\n\t\t\t\t// Message Push in Chat\n\t\t\t\tthis.messages.push({\n\t\t\t\t\timage: require('@assets/images/users/avatar-7.jpg'),\n\t\t\t\t\tid: id + 1,\n\t\t\t\t\tname: 'Shreyu',\n\t\t\t\t\tmessage,\n\t\t\t\t\ttime: currentDate.getHours() + ':' + currentDate.getMinutes(),\n\t\t\t\t})\n\t\t\t}\n\t\t\tthis.submitform = false\n\t\t\tthis.chats = {}\n\t\t},\n\t},\n}\n</script>\n\n<template>\n\t<div class=\"card\">\n\t\t<div class=\"card-body pt-2\">\n\t\t\t<b-dropdown\n\t\t\t\tclass=\"mt-2 float-right\"\n\t\t\t\tvariant=\"black\"\n\t\t\t\tright\n\t\t\t\ttoggle-class=\"p-0 arrow-none text-muted\"\n\t\t\t>\n\t\t\t\t<template v-slot:button-content>\n\t\t\t\t\t<i class=\"uil uil-ellipsis-v\"></i>\n\t\t\t\t</template>\n\n\t\t\t\t<!-- item-->\n\t\t\t\t<b-dropdown-item href=\"javascript:void(0);\">\n\t\t\t\t\t<i class=\"uil uil-refresh mr-2\"></i>Refresh\n\t\t\t\t</b-dropdown-item>\n\t\t\t\t<!-- item-->\n\t\t\t\t<b-dropdown-item href=\"javascript:void(0);\">\n\t\t\t\t\t<i class=\"uil uil-user-plus mr-2\"></i>Add Member\n\t\t\t\t</b-dropdown-item>\n\t\t\t\t<b-dropdown-divider></b-dropdown-divider>\n\t\t\t\t<!-- item-->\n\t\t\t\t<b-dropdown-item href=\"javascript:void(0);\" variant=\"danger\">\n\t\t\t\t\t<i class=\"uil uil-exit mr-2\"></i>Edit\n\t\t\t\t</b-dropdown-item>\n\t\t\t</b-dropdown>\n\t\t\t<h5 class=\"mb-4 header-title\">{{ title }}</h5>\n\n\t\t\t<VuePerfectScrollbar :style=\"`max-height:${chatWindowHeight}`\">\n\t\t\t\t<div class=\"chat-conversation\">\n\t\t\t\t\t<ul class=\"conversation-list\">\n\t\t\t\t\t\t<li\n\t\t\t\t\t\t\tv-for=\"(chat, index) in messages\"\n\t\t\t\t\t\t\t:key=\"chat.id\"\n\t\t\t\t\t\t\t:class=\"{ odd: index % 2 }\"\n\t\t\t\t\t\t\tclass=\"clearfix\"\n\t\t\t\t\t\t>\n\t\t\t\t\t\t\t<div class=\"chat-avatar\">\n\t\t\t\t\t\t\t\t<img :src=\"`${chat.image}`\" alt=\"male\" />\n\t\t\t\t\t\t\t\t<i>{{ chat.time }}</i>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<div class=\"conversation-text\">\n\t\t\t\t\t\t\t\t<div class=\"ctext-wrap\">\n\t\t\t\t\t\t\t\t\t<i>{{ chat.name }}</i>\n\t\t\t\t\t\t\t\t\t<p>{{ chat.message }}</p>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</li>\n\t\t\t\t\t</ul>\n\t\t\t\t</div>\n\t\t\t</VuePerfectScrollbar>\n\t\t\t<form @submit.prevent=\"saveMessage\">\n\t\t\t\t<div class=\"row mt-3\">\n\t\t\t\t\t<div class=\"col-lg-9\">\n\t\t\t\t\t\t<input\n\t\t\t\t\t\t\tid=\"message\"\n\t\t\t\t\t\t\tv-model=\"chats.message\"\n\t\t\t\t\t\t\ttype=\"text\"\n\t\t\t\t\t\t\tclass=\"form-control\"\n\t\t\t\t\t\t\tplaceholder=\"Enter your text\"\n\t\t\t\t\t\t\tname=\"message\"\n\t\t\t\t\t\t\t:class=\"{ 'is-invalid': submitform && $v.chats.message.$error }\"\n\t\t\t\t\t\t/>\n\t\t\t\t\t\t<div\n\t\t\t\t\t\t\tv-if=\"submitform && !$v.chats.message.required\"\n\t\t\t\t\t\t\tclass=\"invalid-feedback\"\n\t\t\t\t\t\t\t>This value is required.</div\n\t\t\t\t\t\t>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div class=\"col-lg-3\">\n\t\t\t\t\t\t<button type=\"submit\" class=\"btn btn-danger chat-send btn-block\"\n\t\t\t\t\t\t\t>Send</button\n\t\t\t\t\t\t>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<!-- end row -->\n\t\t\t</form>\n\t\t\t<!-- end form -->\n\t\t</div>\n\t</div>\n</template>\n"],"sourceRoot":"src/components"}]